<div class="row">
    <div class="col-md-4 page-controls  ">

        <a class="btn btn-default <% if (!previousPage) { %>disabled<% } %>" href="/browse?page=<%= previousPage %>">Previous</a>

        <%= page %> / <%= totalPages %>
